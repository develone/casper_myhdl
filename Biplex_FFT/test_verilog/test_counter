#! /usr/local/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7-7-g9b38de5)";
:vpi_time_precision - 11;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x22fb320 .scope module, "tb_counter" "tb_counter" 2 1;
 .timescale -9 -11;
v0x23103f0_0 .var "clk", 0 0;
v0x2310490_0 .net "count_out", 16 0, v0x2310120_0; 1 drivers
v0x2310540_0 .var "ena", 0 0;
v0x23105f0_0 .var "rst", 0 0;
v0x23106d0_0 .var "updown", 0 0;
S_0x22fb410 .scope module, "dut" "counter" 2 39, 3 8, S_0x22fb320;
 .timescale -9 -11;
v0x22e8e00_0 .net "clk", 0 0, v0x23103f0_0; 1 drivers
v0x2310080_0 .var "cnt", 16 0;
v0x2310120_0 .var/s "count_out", 16 0;
v0x23101c0_0 .net "ena", 0 0, v0x2310540_0; 1 drivers
v0x2310270_0 .net "rst", 0 0, v0x23105f0_0; 1 drivers
v0x2310310_0 .net "updown", 0 0, v0x23106d0_0; 1 drivers
E_0x22d7e80 .event posedge, v0x22e8e00_0;
S_0x22e8d10 .scope begin, "COUNTER_COUNTER_LOGIC" "COUNTER_COUNTER_LOGIC" 3 30, 3 30, S_0x22fb410;
 .timescale -9 -11;
    .scope S_0x22fb410;
T_0 ;
    %wait E_0x22d7e80;
    %fork t_1, S_0x22e8d10;
    %jmp t_0;
    .scope S_0x22e8d10;
t_1 ;
    %load/v 8, v0x2310270_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_0.0, 4;
    %ix/load 0, 17, 0;
    %assign/v0 v0x2310080_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x23101c0_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_0.2, 4;
    %load/v 8, v0x2310310_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_0.4, 4;
    %load/v 8, v0x2310080_0, 17;
    %mov 25, 0, 1;
    %cmpi/u 8, 19, 18;
    %jmp/0xz  T_0.6, 4;
    %ix/load 0, 17, 0;
    %assign/v0 v0x2310080_0, 0, 0;
    %jmp T_0.7;
T_0.6 ;
    %load/v 8, v0x2310080_0, 17;
    %mov 25, 0, 15;
    %addi 8, 1, 32;
    %ix/load 0, 17, 0;
    %assign/v0 v0x2310080_0, 0, 8;
T_0.7 ;
    %jmp T_0.5;
T_0.4 ;
    %load/v 8, v0x2310080_0, 17;
    %mov 25, 0, 1;
    %cmpi/u 8, 21, 18;
    %jmp/0xz  T_0.8, 4;
    %movi 8, 200, 17;
    %ix/load 0, 17, 0;
    %assign/v0 v0x2310080_0, 0, 8;
    %jmp T_0.9;
T_0.8 ;
    %load/v 8, v0x2310080_0, 17;
    %mov 25, 0, 15;
    %subi 8, 1, 32;
    %ix/load 0, 17, 0;
    %assign/v0 v0x2310080_0, 0, 8;
T_0.9 ;
T_0.5 ;
T_0.2 ;
T_0.1 ;
    %load/v 8, v0x2310080_0, 17;
    %ix/load 0, 17, 0;
    %assign/v0 v0x2310120_0, 0, 8;
    %end;
    .scope S_0x22fb410;
t_0 %join;
    %jmp T_0;
    .thread T_0;
    .scope S_0x22fb320;
T_1 ;
    %vpi_call 2 5 "$dumpfile", "tb_counter.vcd";
    %vpi_call 2 6 "$dumpvars", 1'sb0, S_0x22fb320;
    %end;
    .thread T_1;
    .scope S_0x22fb320;
T_2 ;
    %set/v v0x23106d0_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x22fb320;
T_3 ;
    %delay 2700, 0;
    %set/v v0x23106d0_0, 1, 1;
    %delay 1100, 0;
    %set/v v0x23106d0_0, 0, 1;
    %delay 2900, 0;
    %set/v v0x23106d0_0, 1, 1;
    %delay 10000, 0;
    %vpi_call 2 15 "$stop";
    %end;
    .thread T_3;
    .scope S_0x22fb320;
T_4 ;
    %set/v v0x23105f0_0, 0, 1;
    %end;
    .thread T_4;
    .scope S_0x22fb320;
T_5 ;
    %delay 1700, 0;
    %set/v v0x23105f0_0, 1, 1;
    %delay 1100, 0;
    %set/v v0x23105f0_0, 0, 1;
    %delay 2900, 0;
    %set/v v0x23105f0_0, 1, 1;
    %delay 1100, 0;
    %set/v v0x23105f0_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 24 "$stop";
    %end;
    .thread T_5;
    .scope S_0x22fb320;
T_6 ;
    %set/v v0x2310540_0, 0, 1;
    %end;
    .thread T_6;
    .scope S_0x22fb320;
T_7 ;
    %delay 1700, 0;
    %set/v v0x2310540_0, 1, 1;
    %end;
    .thread T_7;
    .scope S_0x22fb320;
T_8 ;
    %set/v v0x23103f0_0, 0, 1;
    %end;
    .thread T_8;
    .scope S_0x22fb320;
T_9 ;
    %delay 500, 0;
    %load/v 8, v0x23103f0_0, 1;
    %inv 8, 1;
    %set/v v0x23103f0_0, 8, 1;
    %jmp T_9;
    .thread T_9;
    .scope S_0x22fb320;
T_10 ;
    %vpi_call 2 47 "$monitor", "At time %t, count_out  = %h (%0d)", $time, v0x2310490_0, v0x2310490_0;
    %end;
    .thread T_10;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_counter.v";
    "../counter.v";
